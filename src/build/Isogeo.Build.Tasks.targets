<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <IsogeoMSBuildTasksPath Condition="'$(IsogeoMSBuildTasksPath)' == ''">$(MSBuildExtensionsPath)\IsogeoMSBuildTasks</IsogeoMSBuildTasksPath>
    <IsogeoMSBuildTasksLib>$(IsogeoMSBuildTasksPath)\Isogeo.Build.Tasks.dll</IsogeoMSBuildTasksLib>
  </PropertyGroup>

  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.FileInfo" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.FxCop" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.MSTest" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.NuGetInstall" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.PartCover" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.SetEnvironmentFromBatch" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.SetEnvironmentVariable" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.VersionInfo" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.XmlSchemasValidate" />
  <UsingTask AssemblyFile="$(IsogeoMSBuildTasksLib)" TaskName="Isogeo.Build.Tasks.Xsd2Code" />
</Project>
